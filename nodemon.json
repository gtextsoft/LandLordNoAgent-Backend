{
  "watch": [
    "server.js",
    "routes",
    "models",
    "middleware",
    "utils",
    "controllers"
  ],
  "ignore": [
    "node_modules",
    "uploads",
    "*.test.js",
    "*.spec.js"
  ],
  "ext": "js,json",
  "env": {
    "NODE_ENV": "development"
  },
  "verbose": false,
  "restartable": "rs",
  "delay": "1000"
}

